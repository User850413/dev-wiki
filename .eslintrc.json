{
  "extends": "next/core-web-vitals",
  "plugins": ["import"],
  "rules": {
    "eslint-plugin-import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index",
          "object",
          "type"
        ]
      }
    ]
  }
}

